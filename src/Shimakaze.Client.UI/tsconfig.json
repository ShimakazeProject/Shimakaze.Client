{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "types": [
      "vite/client"
    ],
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@c/*": [
        "./src/components/*"
      ],
      "@p/*": [
        "./src/pages/*"
      ],
      "@a/*": [
        "./src/apis/*"
      ],
      "@m/*": [
        "./src/models/*"
      ]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}